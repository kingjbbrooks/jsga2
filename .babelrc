{
  "presets": [
    "react",
    "es2015"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-flow-strip-types"
  ],
  "env": {
    "test": {
      "plugins": [
        "istanbul"
      ]
    },
    "production": {
      "plugins": [
        "transform-class-properties",
        "transform-flow-strip-types"
      ],
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ]
    }
  }
}
